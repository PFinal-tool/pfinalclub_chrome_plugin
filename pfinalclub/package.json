{
  "name": "friday-go-extension",
  "version": "1.0.0",
  "description": "Chrome extension for Friday-Go Archives",
  "private": true,
  "scripts": {
    "build:css": "tailwindcss -i ./src/css/style.css -o ./dist/style.css --minify",
    "watch:css": "tailwindcss -i ./src/css/style.css -o ./dist/style.css --watch",
    "build": "npm run build:css",
    "dev": "concurrently \"npm run watch:css\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
